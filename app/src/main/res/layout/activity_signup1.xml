<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<androidx.constraintlayout.widget.ConstraintLayout
		android:layout_width="match_parent"
		android:layout_height="70dp">
		<ImageButton
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			app:srcCompat="?attr/actionModeCloseDrawable"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			android:id="@+id/ib_back"
			app:layout_constraintHorizontal_bias="0.05"/>
		<TextView
			android:text="회원가입"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			android:id="@+id/tv_title"/>
	
	</androidx.constraintlayout.widget.ConstraintLayout>
	<androidx.constraintlayout.widget.ConstraintLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintEnd_toEndOf="parent">
		
		<EditText
			android:layout_width="250dp"
			android:layout_height="wrap_content"
			android:inputType="text"
			android:ems="10"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			android:id="@+id/et_email"
			app:layout_constraintVertical_bias="0.1"/>
		<TextView
			android:text="아이디"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			app:layout_constraintStart_toStartOf="@+id/et_email"
			app:layout_constraintEnd_toEndOf="parent"
			android:id="@+id/textView68"
			app:layout_constraintBottom_toTopOf="@+id/et_email"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintVertical_bias="1.0"
			android:layout_marginBottom="20dp"/>
		<TextView
			android:text="비밀번호"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			app:layout_constraintStart_toStartOf="@+id/et_email"
			app:layout_constraintEnd_toEndOf="@+id/et_email"
			android:id="@+id/textView69"
			app:layout_constraintTop_toBottomOf="@+id/et_email"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintVertical_bias="0.0"
			android:layout_marginTop="20dp"/>
		<EditText
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:inputType="textPassword"
			android:ems="10"
			app:layout_constraintStart_toStartOf="@+id/et_email"
			app:layout_constraintEnd_toEndOf="@+id/et_email"
			android:id="@+id/et_pw"
			app:layout_constraintTop_toBottomOf="@+id/textView69"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintVertical_bias="0.0"
			android:layout_marginTop="20dp"/>
		<TextView
			android:text="비밀번호 확인"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			app:layout_constraintStart_toStartOf="@+id/et_pw"
			app:layout_constraintEnd_toEndOf="@+id/et_pw"
			android:id="@+id/textView70"
			app:layout_constraintTop_toBottomOf="@+id/et_pw"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintVertical_bias="0.0"
			android:layout_marginTop="20dp"/>
		<EditText
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:inputType="textPassword"
			android:ems="10"
			app:layout_constraintStart_toStartOf="@+id/et_pw"
			app:layout_constraintEnd_toEndOf="@+id/et_pw"
			android:id="@+id/et_checkpw"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/textView70"
			app:layout_constraintVertical_bias="0.0"
			android:layout_marginTop="20dp"/>
		<TextView
			android:text="약관 동의"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView71"
			app:layout_constraintStart_toStartOf="@+id/et_checkpw"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/et_checkpw"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintVertical_bias="0.0"
			android:layout_marginTop="40dp"/>
		<CheckBox
			android:text="전체 동의"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/cb_all"
			app:layout_constraintStart_toStartOf="@+id/textView71"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/textView71"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintVertical_bias="0.0"
			android:layout_marginTop="10dp"/>
		<CheckBox
			android:text="[필수]약관 내용"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/cb_1"
			app:layout_constraintStart_toStartOf="@+id/cb_all"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/cb_all"
			app:layout_constraintVertical_bias="0.0"
			android:layout_marginTop="10dp"
			app:layout_constraintHorizontal_bias="0.1"/>
		<CheckBox
			android:text="[필수]약관 내용"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/cb_2"
			app:layout_constraintStart_toStartOf="@+id/cb_1"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/cb_1"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintVertical_bias="0.0"
			android:layout_marginTop="10dp"/>
		<Button
			android:text="다음"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			app:layout_constraintBottom_toBottomOf="parent"
			android:id="@+id/bt_next"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintVertical_bias="1.0"/>
	</androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>